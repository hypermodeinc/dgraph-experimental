{
  "name": "@hypermode/csvkit-monorepo",
  "version": "0.0.0",
  "packageManager": "pnpm@10.6.0",
  "scripts": {
    "dev:frontend": "pnpm --filter \"@hypermode/csvkit-frontend\" dev",
    "dev:backend": "cd ./modus && modus dev",
    "build": "pnpm -r build",
    "build:packages": "pnpm --filter \"!@hypermode/csvkit-frontend\" -r build",
    "test:packages": "pnpm --filter \"!@hypermode/csvkit-integration-tests\" -r test",
    "test:integration": "pnpm --filter \"@hypermode/csvkit-integration-tests\" test"
  },
  "devDependencies": {
    "@babel/cli": "7.24.8",
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.4",
    "@babel/preset-typescript": "7.24.7",
    "@babel/runtime": "7.26.10",
    "@jest/globals": "29.7.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.0.0",
    "@types/node": "22.4.1",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "jest": "^29.0.0",
    "typescript": "5.8.3",
    "next": "^15.3.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "typescript",
        "ts-node",
        "@tsconfig/*",
        "@types/*",
        "jest",
        "@jest/*",
        "supertest",
        "prettier",
        "webpack"
      ]
    }
  }
}
