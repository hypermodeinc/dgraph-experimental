{
  "name": "@hypermode/csvkit-monorepo",
  "version": "0.0.0",
  "packageManager": "pnpm@10.6.0",
  "scripts": {
    "dev:frontend": "pnpm --filter \"@hypermode/csvkit-frontend\" dev",
    "dev:backend": "cd ./modus && modus dev",
    "build": "pnpm -r build",
    "build:packages": "pnpm --filter \"!@hypermode/csvkit-frontend\" -r build",
    "test:packages": "pnpm --filter \"!@hypermode/csvkit-integration-tests\" -r test",
    "test:integration": "pnpm --filter \"@hypermode/csvkit-integration-tests\" test"
  },
  "devDependencies": {
    "@babel/cli": "7.24.8",
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.4",
    "@babel/preset-typescript": "7.24.7",
    "@babel/runtime": "7.25.6",
    "@graphql-eslint/eslint-plugin": "^4.3.0",
    "@jest/globals": "29.7.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.0.0",
    "@types/node": "22.4.1",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@typescript-eslint/parser": "8.0.1",
    "@typescript-eslint/typescript-estree": "8.0.1",
    "assemblyscript": "^0.27.34",
    "assemblyscript-prettier": "^3.0.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.8.0",
    "eslint-plugin-n": "17.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "jest": "^29.0.0",
    "prettier": "3.3.3",
    "typescript": "5.8.3",
    "next": "^15.3.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "typescript",
        "ts-node",
        "@tsconfig/*",
        "@types/*",
        "jest",
        "@jest/*",
        "supertest",
        "prettier",
        "webpack"
      ]
    }
  }
}
